    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "nodemon": "^3.0.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0"
  }
}
