{
  "name": "overwatch-api-client",
  "version": "1.0.0",
  "description": "Overwatch API client using Hono.js, Zod, and Got",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
     "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "vercel-build": "npm run build"
  },
  "keywords": [
    "overwatch",
    "api",
    "hono",
    "zod",
    "got"
  ],
  "author": "Yassine",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.19.0",
    "defer-to-connect": "^2.0.1",
